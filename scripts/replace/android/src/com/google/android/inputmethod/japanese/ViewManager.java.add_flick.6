    // Enter
    if (primaryCode == keycodeEnter) {
      return KeycodeConverter.SPECIALKEY_ENTER;
    }

    // Tab
    if (primaryCode == keycodeTab) {
      return KeycodeConverter.SPECIALKEY_TAB;
    }

    // Backspace
    if (primaryCode == keycodeBackspace) {
      return KeycodeConverter.SPECIALKEY_BACKSPACE;
    }

    // Escape
    if (primaryCode == keycodeEscape) {
      return KeycodeConverter.SPECIALKEY_ESCAPE;
    }

